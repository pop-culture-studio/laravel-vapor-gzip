{
    "name": "pop-culture-studio/laravel-vapor-gzip",
    "type": "library",
    "description": "Gzip Response middleware for Laravel Vapor",
    "keywords": ["laravel", "vapor", "gzip"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-zlib": "*",
        "illuminate/http": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PopCultureStudio\\Vapor\\Middleware\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
