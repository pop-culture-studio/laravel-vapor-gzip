{
    "name": "puklipo/laravel-vapor-gzip",
    "type": "library",
    "description": "Gzip Response middleware for Laravel Vapor",
    "keywords": ["laravel", "vapor", "gzip"],
    "license": "MIT",
    "authors": [
        {
            "name": "PCS",
            "email": "pcs.engineer.team@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-zlib": "*",
        "illuminate/http": "^8.0||^9.0||^10.0"
    },
    "autoload": {
        "psr-4": {
            "Puklipo\\Vapor\\Middleware\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
